<h1><%= @list.name %></h1>

<%= link_to "Back to your lists", lists_path %>

<div>
  <% @list.bookmarks.each do |bookmark| %>
    <div class="card-trip">
      <img src=<%=bookmark.movie.poster_url%> />
      <div class="card-trip-infos">
        <div>
          <h2><%=bookmark.movie.name%></h2>
          <p><%=bookmark.movie.overview%></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
